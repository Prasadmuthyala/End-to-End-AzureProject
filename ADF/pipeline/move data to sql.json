{
	"name": "move data to sql",
	"properties": {
		"activities": [
			{
				"name": "flight Copy data from data lake to sql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "sampleDatasets/flights2",
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"Date"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference",
						"parameters": {
							"table": "dim_flight"
						}
					}
				]
			},
			{
				"name": "airport Copy data from data lake to sql_copy1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "sampleDatasets/airport",
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBatchSize": 10000,
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"code"
							]
						},
						"sqlWriterUseTableLock": true,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "code",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "code",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "description",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "description",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "city",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "city",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "state",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "state",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "airport_name",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "airport_name",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference",
						"parameters": {
							"table": "dim_airport"
						}
					}
				]
			},
